console.log(makeRectangle(5, 10));

function makeRectangle(width, height) {
  let rectangle = "";
  for (let i = 0; i < height; i++) {
    for (let j = 0; j < width; j++) {
        rectangle = rectangle + "*";
       
//console.log("*");
    }
    j = 0;
    

console.log(rectangle);
rectangle = "";
//    console.log("*")
//    rectangle += (makeLine(width) + '\n');
  }
//  return rectangle.slice(0, -1);
  
}
makeRectangle();